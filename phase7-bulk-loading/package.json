{
  "name": "phase7-bulk-loading",
  "version": "1.0.0",
  "description": "Phase 7: Bulk Loading - Data Migration System for 700K+ dockets",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "prepare": "npm run build",
    "test": "jest",
    "migrate:prepare": "ts-node src/scripts/prepare-data.ts",
    "migrate:validate": "ts-node src/scripts/validate-data.ts",
    "migrate:import": "ts-node src/scripts/bulk-import.ts",
    "migrate:verify": "ts-node src/scripts/verify-import.ts",
    "generate:tags": "ts-node src/scripts/generate-tags.ts",
    "cleanup": "ts-node src/scripts/cleanup.ts"
  },
  "keywords": [
    "bulk-loading",
    "data-migration",
    "rfid",
    "dockets",
    "evidence-management"
  ],
  "author": "RFID Tracking System",
  "license": "MIT",
  "dependencies": {
    "csv-parser": "^3.2.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^17.2.1",
    "express": "^4.18.2",
    "fast-csv": "^4.3.6",
    "jsonstream": "^1.0.0",
    "pg": "^8.16.3",
    "pg-pool": "^3.7.0",
    "progress": "^2.0.3",
    "redis": "^4.6.10",
    "uuid": "^9.0.1",
    "winston": "^3.17.0",
    "worker-threads": "^1.0.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/csv-parser": "^1.2.0",
    "@types/express": "^5.0.3",
    "@types/node": "^24.1.0",
    "@types/pg": "^8.15.5",
    "@types/progress": "^2.0.7",
    "@types/uuid": "^9.0.7",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}